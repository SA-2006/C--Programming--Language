#include <stdio.h>
#include <math.h>
int main()
{
int x,y,z;


printf("Enter three number to find the greatest one:\n");
scanf(" %d%d%d",&x,&y,&z);

if (x>y)
{
	if (x>z)
	{ printf("The greatest number is: %d\n", x); }
	else
	{ printf("The greatest number is: %d\n", z); }
}
else 
{
	if (y>z)
	{ printf("The greatest number is: %d\n", y); }
	else
	{ printf("The greatest number is: %d\n", z); }	
}


return 0;
}
