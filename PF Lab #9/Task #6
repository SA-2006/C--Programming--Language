#include <stdio.h>

float average(int a, int b, int c)
{
    float avg;
    avg = (a + b + c) / 3;  
    return avg;               
}

int main()
{
    int x = 10, y = 20, z = 30;
    float result;

    result = average(x, y, z);

    printf("The average of %d, %d, and %d is: %.2f\n", x, y, z, result);

    return 0;
}
