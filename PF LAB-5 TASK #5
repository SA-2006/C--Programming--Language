#include <stdio.h>
#include <math.h>
int main()
{
int coursenumber;
char grade;

printf("Enter your course number(1-3)\n");
scanf(" %d",&coursenumber);

switch(coursenumber)
{
	case 1:
		printf("Enter your grade in this course(A/B/C/D/F)\n");
		scanf(" %c",&grade);
		switch(grade)
		{
		case 'A':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		case 'B':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		case 'C':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		case 'D':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		case 'F':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		default:
		printf("Incorrect grade entered");	
		}
	break;	
	case 2:
		printf("Enter your grade in this course(A/B/C/D/F)\n");
		scanf(" %c",&grade);
		switch(grade)
		{
		case 'A':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		case 'B':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		case 'C':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		case 'D':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		case 'F':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		default:
		printf("Incorrect grade entered");	
		}
	break;			
	case 3:
		printf("Enter your grade in this course(A/B/C/D/F)\n");
		scanf(" %c",&grade);
		switch(grade)
		{
		case 'A':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		case 'B':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		case 'C':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		case 'D':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		case 'F':
		printf("You scored %c grade in your course number %d",grade,coursenumber);
		break;
		default:
		printf("Incorrect grade entered");	
		}
	break;	
	default:
	printf("Incorrect course number entered");	
}
}
