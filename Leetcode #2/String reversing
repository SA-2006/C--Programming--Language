#include <stdio.h>
#include <string.h>
int main()
{
	char str[50], strn[50];
	int len;
	printf("Input a string to reverse its order. \n");
	fgets(str, sizeof(str), stdin);
	len=strlen(str)-1;
//	printf("%d\n",len);
	
	for(int i=0; i<len; i++)
	{
		strn[len-i]=str[i];
	}
	printf("Reversed order: \n");
	printf("%s",strn);
return 0;
}
