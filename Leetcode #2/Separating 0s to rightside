#include <stdio.h>
int main()
{
	int len;
	printf("Enter the length of array: \n");
	scanf("%d", &len);	
	int arr[len]={0};
	printf("\nEnter the elements of array: \n");
	for(int i=0; i<len; i++)
	{
		scanf("%d",&arr[i]);
	}
	
	printf("Your array before was: \n");
	for(int b=0; b<len; b++)
	{
		printf("%d ",arr[b]);
	}
	
	int temp;
	for(int j=0; j<(len-1); j++)
	{
		if(arr[j]==0)
		{
			for(int k=j; k<(len-1); k++)
			{
				temp=arr[k];
				arr[k]=arr[k+1];
				arr[k+1]=temp;
			}
		}
	}
	
	printf("\nYour array now is: \n");
	for(int a=0; a<len; a++)
	{
		printf("%d ",arr[a]);
	}
return 0;
}
